{
  "name": "bot-cms-server",
  "version": "0.0.0",
  "private": true,
  "main": "server.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run build && node dist/server.js",
    "start:dev": "concurrently --handle-input -k -p \"[{name}]\" -n \"Node,TypeScript\" -c \"yello.bold, cyan.bold\"  \"wait-on dist/server.js && npm run watch-node\" \"npm run watch-ts\" ",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "watch-node": "nodemon",
    "watch-ts": "tsc -w -p tsconfig.build.json"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.1.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "errorhandler": "^1.5.1",
    "express": "~4.17.3",
    "fb-messenger-bot-api": "^4.3.1",
    "http-errors": "^1.7.2",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.0",
    "morgan": "~1.9.1",
    "node-nlp": "^3.1.1",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/http-errors": "^1.6.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.134",
    "@types/mongoose": "^5.5.6",
    "@types/morgan": "^1.7.35",
    "concurrently": "^4.1.0",
    "dotenv": "^8.0.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "3.8.0",
    "typescript": "^3.5.2",
    "wait-on": "^3.2.0"
  }
}
